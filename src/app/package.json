{
    "name": "kestrel-web",
    "version": "0.1.0",
    "description": "Kestrel - Easy File Encryption",
    "type": "module",
    "private": true,
    "exports": {
        "./keyring": "./src/js/keyring.js"
    },
    "scripts": {
        "build": "esbuild app.bundle=src/js/index.js worker.bundle=src/js/worker.js --format=iife --target=esnext --bundle --outdir=dist --outbase=src/js --loader:.js=jsx --jsx=automatic --define:process.env.NODE_ENV=\\\"development\\\"",
        "dev": "esbuild app.bundle=src/js/index.js worker.bundle=src/js/worker.js --format=iife --target=esnext --bundle --outdir=dist --outbase=src/js --loader:.js=jsx --jsx=automatic --define:process.env.NODE_ENV=\\\"development\\\" --watch",
        "prod": "esbuild app.bundle=src/js/index.js worker.bundle=src/js/worker.js --format=iife --target=esnext --bundle --outdir=dist --outbase=src/js --loader:.js=jsx --jsx=automatic --define:process.env.NODE_ENV=\\\"production\\\"",
        "test": "node --test tests",
        "copy": "node copy.js"
    },
    "repository": {
        "type": "git",
        "url": "https://githbub.com/finfet/kestrel-js"
    },
    "keywords": [
        "encryption",
        "security",
        "privacy",
        "file"
    ],
    "author": "Kyle Schreiber <kyle@80x24.net>",
    "license": "MIT",
    "dependencies": {
        "kestrel-crypto": "0.1.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "esbuild": "0.20.2"
    }
}
