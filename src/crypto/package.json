{
    "name": "kestrel-crypto",
    "version": "0.1.0",
    "description": "Kestrel crypto library",
    "type": "module",
    "private": true,
    "exports": {
        ".": "./dist/kestrel-crypto.bundle.js",
        "./utils": "./src/utils.js"
    },
    "scripts": {
        "build": "esbuild kestrel-crypto.bundle=src/crypto.js --bundle --target=esnext --format=esm --outdir=dist --loader:.wasm=base64",
        "dev": "esbuild kestrel-crypto.bundle=src/crypto.js --bundle --target=esnext --format=esm --outdir=dist --loader:.wasm=base64 --watch",
        "test": "node --test tests",
        "prepare": "esbuild kestrel-crypto.bundle=src/crypto.js --bundle --target=esnext --format=esm --outdir=dist --loader:.wasm=base64"
    },
    "repository": {
        "type": "git",
        "url": "https://githbub.com/finfet/kestrel-js"
    },
    "keywords": [
        "encryption"
    ],
    "author": "Kyle Schreiber <kyle@80x24.net>",
    "license": "MIT",
    "devDependencies": {
        "esbuild": "0.20.2"
    }
}
